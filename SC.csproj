    <Reference Include="RAR.IdCard.Sdk.Reader">
      <HintPath>Lib\RAR.IdCard.Sdk.Reader.dll</HintPath>
    </Reference>
    <Reference Include="RAR.IdCard.Sdk.Utils.TKCrypto">
      <HintPath>Lib\RAR.IdCard.Sdk.Utils.TKCrypto.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
